levels(Pok2001.2022$Annee)
table(Pok2001.2022$Annee)
# Présentation de la fonction "ctree" ("categorical tree") avec
# un modèle de température en fonction du mois de l'année
partykit::ctree(Temp_eau_YSI_degC~Mois, data=Pok2001.2022)
# Ces divisions binaires sont-elles "statistiquement signficatives"?
ctree01_temp_mois=partykit::ctree(Temp_eau_YSI_degC~Mois, data=Pok2001.2022,
control=partykit::ctree_control(minsplit=1, minbucket=1,minprob=0.1,
maxdepth=1, testtype="Teststatistic"))
ctree01_temp_mois
sctest(ctree01_temp_mois)
# À faire: comprendre pourquoi les valeurs p ne sont pas calculées
plot(ctree01_temp_mois)
# À faire: comprendre pourquoi les valeurs p ne sont pas calculées
plot(ctree01_temp_mois)
# On peut ajouter plusieurs variables indépendantes "X" au modèle:
ctree02_temp=partykit::ctree(Temp_eau_YSI_degC~ Mois + Period, data=Pok2001.2022,
control=partykit::ctree_control(testtype="Teststatistic"))
ctree02_temp
# La première division se fait selon les mois: le mois de juillet se distinguent des mois
# d'août et septembre et ceux-ci se distinguent des mois d'octobre et novembre.
# Dans le mois de juillet, la période 1 a été plus chaude (16,2 deg. C en moyenne) 
# que la période 2 (13,6 deg. C).
sctest(ctree02_temp)
plot(ctree02_temp)
